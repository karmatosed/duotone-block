{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#333333",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#6c6c6c",
					"name": "Contrast / Two",
					"slug": "contrast-2"
				},
				{
					"color": "#6e6e6e",
					"name": "Contrast / Three",
					"slug": "contrast-3"
				},
				{
					"color": "#444444",
					"name": "Contrast / Four",
					"slug": "contrast-4"
				},
				{
					"color": "#c4c4c4",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#919191",
					"name": "Accent / Two",
					"slug": "accent-2"
				},
				{
					"color": "#afb1b2",
					"name": "Accent / Three",
					"slug": "accent-3"
				}
			]
		},
		"layout": {
			"contentSize": "560px",
			"wideSize": "840px"
		},
		"spacing": {
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Lucida Fax, Georgia, Helvetica, sans-serif",
					"name": "Body Font",
					"slug": "body-font"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}
